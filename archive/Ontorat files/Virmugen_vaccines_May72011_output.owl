        <obo:IAO_0000117>Rebecca Racz, Yongqun He</obo:IAO_0000117>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/VO_0002970 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/VO_0002970">
