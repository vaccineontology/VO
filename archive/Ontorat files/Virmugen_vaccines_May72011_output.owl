        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/VO_0002512"/>
        <rdfs:comment>An M2-2 mutant of human respiratory syncytial virus A2 is attenuated in African green monkeys. When challenged with wild type RSV, two doses provided complete protection against challenge in the lower respiratory tract and a significant reduction of the challenge virus in the upper respiratory tract</rdfs:comment>
        <obo:IAO_0000117>Rebecca Racz, Yongqun He</obo:IAO_0000117>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/VO_0002979 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/VO_0002979">
