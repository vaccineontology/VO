        <obo:IAO_0000117>Rebecca Racz, Yongqun He</obo:IAO_0000117>
        <rdfs:seeAlso>NCBI_Gene_ID: 1198671; PMID:  17664263</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/VO_0002565 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/VO_0002565">
