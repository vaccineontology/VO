        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0200200"/>
        <obo:IAO_0000118>Pearson&#39;s Chi square test</obo:IAO_0000118>
        <obo:IAO_0000117>HL, QL, YH</obo:IAO_0000117>
        <obo:IAO_0000118>Pearson&#39;s X^2 test</obo:IAO_0000118>
        <obo:IAO_0000115>A chi square test that is applied to sets of categorical data to evaluate how likely it is that any observed difference between the sets arose by chance. It is suitable for unpaired data from large samples. It is also the most widely used of many chi-squared tests.</obo:IAO_0000115>
        <obo:IAO_0000119>https://en.wikipedia.org/wiki/Pearson%27s_chi-squared_test</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/obcs.owl"/>
