        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0200000"/>
        <obo:IAO_0000115>A data processing that applies a deterministic mathematical function to each point in a data set; that is, each data point zi is replaced with the transformed value yi = f(zi), where f is a function.</obo:IAO_0000115>
        <obo:IAO_0000119>WEB: https://en.wikipedia.org/wiki/Data_transformation_(statistics)</obo:IAO_0000119>
        <obo:IAO_0000117>Jie Zheng, Yongqun He</obo:IAO_0000117>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/obcs.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBCS_0000214 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBCS_0000214">
